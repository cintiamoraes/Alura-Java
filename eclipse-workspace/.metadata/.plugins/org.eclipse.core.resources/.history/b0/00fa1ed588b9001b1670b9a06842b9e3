
public class Cliente implements Autenticavel {//quando vc clica no cliente o Java já auxilai na implementação das obrigações lá da clsss Autenticável

	private AutenticacaoUtil autenticador;
	
	public Cliente() {
		this.autenticador = new AutenticacaoUtil();
	}
	
	//gera os métodos concretos setSenha e autentica
	@Override
	public void setSenha(int senha) {
		this.autenticador.setSenha(senha);
	}

	@Override
	public boolean autentica(int senha) {
		return this.autenticador.autentica(senha);
	}
	
}
