package capituloII;

public class AlcanceDeLasVariables {
	public static void main(String[] args) {
		
		int dias = 0;
		
		while (dias <30) {
			
			dias = dias++;
			int semanas = dias/7;
			
			System.out.println("Han Pasado " + dias + "días lo que supone "+ semanas +" semanas");
					
			}
		
	}

}
